import java.util.Scanner;

public class Exercises {

	public static void main(String[] args) {
		int num;
		System.out.print("Enter a number: ");
		Scanner scan = new Scanner(System.in);
		num = scan.nextInt();
		
		//printEvenNums(num);
		//divisibleNums(num);
		//sumNums(num);
		//pramidPrint(num);
		revIntArray(3);
		
	}
	//Write a for loop to print even number between 1 to 100
	public static void printEvenNums(int n){
		for(int i=0; i<=n; i++){
			if(i%2 == 0){
				System.out.print(i + ", ");
			}
			}
			
		}
	
	public static void divisibleNums(int n){
		for(int i=0; i<=n; i++){
			if(i%5 == 0){
				System.out.println("Divisble by 5 "+ i);
			}
			}
			
		}
	public static void sumNums(int n){
		int sum = 0;
		for(int i=0; i<=n; i++){
			sum = sum + i;
			}
		System.out.println(sum);
		 
		}
	public static void pramidPrint(int n){
		for(int i=1; i<=n; i++){
			for(int j=1; j<=i; j++){
				System.out.print(j );
			}
			System.out.println();
		}
	}
	
	public static void revIntArray(int n){
		int a[] = new int[n];
		a[0] = 11;
		a[1] = 22;
		a[2] = 33;
		
		for(int i=a.length-1; i>=0; i--){
			
			System.out.println(a[i]);
		}
	}
			
}


