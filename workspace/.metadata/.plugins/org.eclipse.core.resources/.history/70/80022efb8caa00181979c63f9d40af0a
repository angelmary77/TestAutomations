
public class Exercises {

	public static void main(String[] args) {
		//1. 
		printEvenNums(100);

	}
	//Write a for loop to print even number between 1 to 100
	public static void printEvenNums(int n){
		int eNum = 0;
		for(int i=0; i<=n; i++){
			if(i%2 == 0){
				System.out.println("Even" + i);
			}else{
				System.out.println("Odd" + i);
			}
			
		}
	}

}
